/back/usbeaufort-api/var/cache/*
/back/usbeaufort-api/var/logs/*
/back/usbeaufort-api/var/sessions/*
!/back/usbeaufort-api/var/cache/.gitkeep
!/back/usbeaufort-api/var/logs/.gitkeep
!/back/usbeaufort-api/var/sessions/.gitkeep

# Logs (Symfony4)
/back/usbeaufort-api/var/log/*
!/back/usbeaufort-api/var/log/.gitkeep

