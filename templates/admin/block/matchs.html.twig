
{% for key, group in phase.meets|filter(v => v.poule == groupe)|group_by(==> _.tour) %}
    <h5 class="mt-4">Tour {{ key }}</h5>
    <table class="table table-striped table-bordered">
        <tr><th>Équipe A</th><th></th><th>Équipe B</th><th>Score A</th><th>Score B</th></tr>
        {% for meet in group %}
            <tr>
                <td>{% if meet.teamA %}{{ meet.teamA.Name }} ({{ meet.scoreA }}){% endif %}</td>
                <td>-</td>
                <td>{% if meet.teamB %}{{ meet.teamB.Name }} ({{ meet.ScoreB }}){% endif %}</td>
                <td>{{ meet.scoreA }}</td>
                <td>{{ meet.ScoreB }}</td>
            </tr>
        {% endfor %}

    </table>

{% endfor %}