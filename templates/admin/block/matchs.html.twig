{% if phase.meets|length > 0 %}
{% for key, group in phase.meets|filter(v => v.poule.id == groupe)|group_by(==> _.tour) %}
    <h5 class="mt-4">Tour {{ key }}</h5>
    <table class="table table-striped table-bordered">
        <tr><th>Équipe A</th><th></th><th>Équipe B</th><th>Score A</th><th>Score B</th></tr>
        {% for meet in group %}
            <tr>
                <td>{% if meet.teamA %}
                        {{ meet.teamA.Name }} ({{ meet.scoreA }})
                        {% elseif phase.param == 24  and meet.positionA.intParam is not null %}3ème de poule ({{ meet.positionA.intParam }}{% if meet.positionA.intParam == 1 %}er{% else %}ème{% endif %} meilleur troisième)
                        {% elseif meet.positionA %}{{ meet.positionA.Rang }}{% if meet.positionA.Rang == 1 %}er{% else %}ème{% endif %} de la poule {{ meet.positionA.pouleFrom }}
                    {% elseif meet.Name %}{{ meet.Name }}{% endif %}</td>
                <td>-</td>
                <td>{% if meet.teamB %}{{ meet.teamB.Name }} ({{ meet.ScoreB }})
                    {% elseif phase.param == 24  and meet.positionB.intParam is not null %}3ème de poule ({{ meet.positionB.intParam }}{% if meet.positionB.intParam == 1 %}er{% else %}ème{% endif %} meilleur troisième)
                    {% elseif meet.positionB %}{{ meet.positionB.Rang }}{% if meet.positionB.Rang == 1 %}er{% else %}ème{% endif %} de la poule {{ meet.positionB.pouleFrom }}
                    {% elseif meet.Name %}{{ meet.Name }}{% endif %}</td>

                <td>{{ meet.scoreA }}</td>
                <td>{{ meet.ScoreB }}</td>
            </tr>
        {% endfor %}

    </table>



{% endfor %}    {% endif %}