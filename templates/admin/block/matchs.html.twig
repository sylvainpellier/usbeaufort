{% if phase.meets|length > 0 %}
{% for key, group in phase.meets|filter(v => v.poule.id == groupe)|group_by(==> _.tour) %}
    <h5 class="mt-4">Tour {{ key }}</h5>
    <table class="table table-striped table-bordered">
        <tr><th>Équipe A</th><th></th><th>Équipe B</th><th>Score A</th><th>Score B</th><th>TAB</th><th>TAB</th><th>Horaire</th><th>Terrain</th><th>Arbitre</th></tr>
        {% for meet in group %}
            <tr class="text-black">
                <td>{% if meet.teamA %}
                        {{ meet.teamA.Name }} {% if meet.scoreA %}({{ meet.scoreA }}){% endif %} {% if meet.PenaltyA %}(tab {{ meet.PenaltyA }}){% endif %}
                        {% elseif phase.param == 24  and meet.positionA is not null and meet.positionA.intParam is not null %}3ème de poule ({{ meet.positionA.intParam }}{% if meet.positionA.intParam is not null and meet.positionA.intParam == 1 %}er{% else %}ème{% endif %} meilleur troisième)
                        {% elseif meet.positionA %}{{ meet.positionA.Rang }}{% if meet.positionA.Rang == 1 %}er{% else %}ème{% endif %} de la poule {{ meet.positionA.pouleFrom }}
                    {% elseif meet.Name %}{{ meet.Name }}{% endif %}</td>
                <td>-</td>
                <td>{% if meet.teamB %}{{ meet.teamB.Name }} ({{ meet.ScoreB }})  {% if meet.PenaltyB %}(tab {{ meet.PenaltyB }}){% endif %}
                    {% elseif phase.param == 24  and meet.positionB is not null  and meet.positionB.intParam is not null %}3ème de poule ({{ meet.positionB.intParam }}{% if meet.positionB.intParam is not null and meet.positionB.intParam == 1 %}er{% else %}ème{% endif %} meilleur troisième)
                    {% elseif meet.positionB %}{{ meet.positionB.Rang }}{% if meet.positionB.Rang == 1 %}er{% else %}ème{% endif %} de la poule {{ meet.positionB.pouleFrom }}
                    {% elseif meet.Name %}{{ meet.Name }}{% endif %}</td>

                <td class="{% if meet.scoreA > meet.ScoreB %} text-bold {% endif %}">{{ meet.scoreA }}</td>
                <td class="{% if meet.ScoreB > meet.scoreA %} text-bold {% endif %}">{{ meet.ScoreB }}</td>
                <td class="{% if meet.PenaltyA > meet.penaltyB %} text-bold {% endif %}">{% if meet.scoreA == meet.scoreB %}{{ meet.PenaltyA }}{% endif %}</td>
                <td class="{% if meet.PenaltyB > meet.penaltyA %} text-bold {% endif %}">{% if meet.scoreA == meet.scoreB %}{{ meet.PenaltyB }}{% endif %}</td>
                <td>{{ meet.time|date("H:i:s") }}</td>
                <td>{{ meet.field }}</td>
                <td><small>{{ meet.arbitre }}</small></td>

            </tr>
        {% endfor %}

    </table>



{% endfor %}    {% endif %}